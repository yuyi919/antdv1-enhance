{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "outDir": "./jsx-runtime",
    "tsBuildInfoFile": "./node_modules/tsconfig/.tsBuildInfo/web-vue-jsx-factory.json"
  },
  "exclude": [
    "src/**/*.spec.ts",
    "src/**/*.test.ts",
    "src/**/*.spec.tsx",
    "src/**/*.test.tsx",
    "**/__test__/**"
  ],
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
