{
  "compilerOptions": {
    "declaration": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@yuyi919/vue-jsx-factory",
    "module": "esnext",
    "target": "es2020",
    "useDefineForClassFields": false,
    "forceConsistentCasingInFileNames": true,
    "rootDirs": ["./src", "./playground"],
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "@yuyi919/vue-antv-plus2": ["./src/index.ts"],
      "@yuyi919/vue-jss": ["./node_modules/@yuyi919/vue-jss/src/index.ts"],
      "@yuyi919/vue-use": ["./node_modules/@yuyi919/vue-use/src/index.ts"]
    }
  },
  "include": ["src", "playground"]
}
