{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["build:notype", "generate", "^build"],
      "outputs": ["dist/**", ".next/**", "lib/**", ".heft"]
    },
    "generate": {
      "dependsOn": ["^generate"],
      "outputs": ["dist/**", "generated/**", "src/**/*.gen.ts", "lib/**"]
    },
    "build:notype": {
      "dependsOn": ["build:pre", "^build:notype"],
      "outputs": ["dist/**", "out/**", "lib/**"]
    },
    "build:pre": {
      "dependsOn": ["^build:pre"],
      "outputs": ["dist/**", "out/**", "lib/**"]
    },
    "clean": {
      "outputs": [],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "dev": {
      "cache": false
    }
  }
}
